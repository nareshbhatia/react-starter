{
  "name": "react-starter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rimraf node_modules dist",
    "dev": "vite",
    "fix": "run-s _lint:fix _format:fix",
    "lint": "run-s _lint _format",
    "preview": "vite preview",
    "_format": "prettier --list-different '*.{js,ts,json,md}' 'src/**/*.{css,js,jsx,ts,tsx,json,md}'",
    "_format:fix": "npm run _format -- --write",
    "_lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "_lint:fix": "npm run _lint -- --fix"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@code-shaper/eslint-config": "^2.*",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "vite": "^6.0.5"
  }
}
